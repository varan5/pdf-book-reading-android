<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pdfbookapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PdfBookApp">
        <activity android:name=".MysqlBookPdf"
            android:label="MySql" />
        <activity android:name=".OopBookPdf"
            android:label="OOP"/>
        <activity android:name=".DSABookPdf"
            android:label="DSA"/>
        <activity android:name=".JavaBookPdf"
            android:label="Java"/>
        <activity android:name=".PythonBookActivity"
            android:label="Python"/>
        <activity android:name=".MotivationalBookActivity"
            android:label="Motivational" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>